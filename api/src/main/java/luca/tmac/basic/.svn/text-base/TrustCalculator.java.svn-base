package luca.tmac.basic;

import java.util.List;

public interface TrustCalculator {
	/**
	 * method for calculate the trustworthiness of a Group of user given a
	 * List of their trustworthiness values
	 * @param memberTrustList list of the trustworthiness of the members of the group
	 * @return trustworthiness value of the group
	 * */
	double calculateTrust(List<Double> memberTrustList);

}
