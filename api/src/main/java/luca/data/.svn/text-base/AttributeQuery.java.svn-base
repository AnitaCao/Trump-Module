package luca.data;


public class AttributeQuery {
	public String name;
	public String value;
	public String type;
	
	public AttributeQuery(String pName,String pValue,String pType)
	{
		name = pName;
		value = pValue;
		type = pType;
	}
	
	@Override
	public String toString()
	{
		return name + " = \"" + value + "\"";
	}
}
